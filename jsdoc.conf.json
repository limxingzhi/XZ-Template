{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(jsx|tsx|ts|js)?$"
  },
  "plugins": [
    "better-docs",
    "node_modules/better-docs/category",
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/component"
  ],
  "typescript": {
    "moduleRoot": "src"
  },
  "templates": {
    "better-docs": {
      "name": "JSDocs for XingZhi's Template"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "readme": "./README.md"
}
